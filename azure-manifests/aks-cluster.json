{
  "apiVersion": "2021-03-01",
  "location": "eastus",
  "name": "myAKSCluster",
  "properties": {
    "dnsPrefix": "myaks",
    "agentPoolProfiles": [
      {
        "name": "agentpool",
        "count": 3,
        "vmSize": "Standard_DS2_v2",
        "osType": "Linux",
        "mode": "System",
        "orchestratorVersion": "1.21.2"
      }
    ],
    "kubernetesVersion": "1.21.2",
    "enableRBAC": true,
    "networkProfile": {
      "networkPlugin": "azure",
      "networkPolicy": "calico"
    },
    "identity": {
      "type": "SystemAssigned"
    }
  },
  "sku": {
    "name": "Basic",
    "tier": "Free"
  },
  "type": "Microsoft.ContainerService/managedClusters"
}